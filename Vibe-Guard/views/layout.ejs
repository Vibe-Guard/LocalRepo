<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/style.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet">
    
    <title>Admin</title>
    <%- defineContent('style')%>
    <%- defineContent("Home-style")%>
    
    <!-- <%- defineContent("Symptom-style")%> -->
</head>
<body>
    


    <div class="wrapper">
        <div id="content">
            <%- include('./partials/navbar') %> 
            <%- body %> 
        </div>

        <div id="sidebar">
            <%- include('./partials/sidebar') %>
        </div>
    </div>

    
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
    <%- defineContent("Home-Script")%>
    <!-- <%- defineContent("Symptom-Script")%> -->
    <script src="/script.js"></script>
    <script>
  document.addEventListener('DOMContentLoaded', function () {
    const navLinks = document.querySelectorAll('#navbarSupportedContent .nav-link');
    const navbarCollapse = document.getElementById('navbarSupportedContent');

    navLinks.forEach(function(link) {
      link.addEventListener('click', function () {
        // Only collapse if navbar is currently shown (expanded)
        if (navbarCollapse.classList.contains('show')) {
          const bsCollapse = bootstrap.Collapse.getInstance(navbarCollapse);
          bsCollapse.hide();
        }
      });
    });
  });
</script>


    
</body>
</html>
